    "dev:web": "turbo run dev --filter=web"
  },
  "devDependencies": {
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "prettier": "^3.2.5",
    "turbo": "^2.0.13",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  }
}
