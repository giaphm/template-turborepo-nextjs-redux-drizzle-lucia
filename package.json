{
  "name": "template-turborepo-nextjs-redux-drizzle-lucia",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky",
    "type-check": "turbo run type-check",
